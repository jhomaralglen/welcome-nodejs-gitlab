{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-underscore-dangle": "off",
      "no-case-declarations": "off",
      "global-require": "off",
      "class-methods-use-this": "off",
      "import/no-dynamic-require": "off"
    }
  }